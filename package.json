{
  "name": "ai-fashion-extractor-backend",
  "version": "1.0.0",
  "description": "Backend API for AI Fashion Attribute Extractor",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "prisma generate && tsc --skipLibCheck && npm run postbuild",
    "postbuild": "node -e \"require('fs').cpSync('src/generated', 'dist/generated', {recursive: true})\"",
    "postinstall": "prisma generate",
    "start": "node --max-old-space-size=512 dist/index.js",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node prisma/seed-fast.ts",
    "db:seed:slow": "ts-node prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:test": "ts-node scripts/test-db-queries.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed-fast.ts"
  },
  "keywords": [
    "ai",
    "fashion",
    "api",
    "openai"
  ],
  "author": "Shubham Kushvanshi",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.17.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "ioredis": "^5.8.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "prisma": "^6.17.1",
    "typescript": "^5.9.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2"
  }
}
